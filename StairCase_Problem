class main{
    public static int StairCase(int n){
        int prev1=1;
        int prev2=1;
        for(int i =2;i<=n;i++){
            int current = prev1+prev2;
            prev2 = prev1;
            prev1 = current;
        }
        return prev1;
    }
    public static void main(String [] args){
        System.out.println("For n = 1 the ways will be "+StairCase(1));
        System.out.println("For n = 2 the ways will be "+StairCase(2));
        System.out.println("For n = 3 the ways will be "+StairCase(3));
        System.out.println("For n = 4 the ways will be "+StairCase(4));
        System.out.println("For n = 5 the ways will be "+StairCase(5));
    }
}
